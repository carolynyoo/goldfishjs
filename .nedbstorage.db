</section> \n\n    <section layout=\"row\" layout-sm=\"column\" layout-align=\"center center\">\n    <label for=KeyframeScrubber>Adjust Playhead</label>\n\t<input type=range id=\"KeyframeScrubber\" min=0 value=20 max=$scope.framesArray.length step=1 ng-model=\"currentFrameID\" ng-change=\"$parent.advanceFrame(currentFrameID, currentFrame)\">\n\t<output for=KeyframeScrubber id='playhead'>{{currentFrameID}}</output>\n    <br/>\n\t<button ng-click=\"broadcastKeyframeSelected()\">Simulate Keyframe Click</button>\n    </section>\n<!--     <fileinfo></fileinfo> -->\n\n    <!-- refactor into ng-repeat later -->\n<!-- \t    <div class=\"panel\">\n    <div class=\"content\">\n    <p>\n        filename: {{fileName}}\n    </p>\n    <p>\n        branchname: {{branchName}}\n    </p>\n    <p>\n        last commit: {{lastCommit}}\n    </p>\n    <p ng-filter>\n        committed at: {{lastCommitTime | datetime}}\n    </p>\n    <p>\n       frame id: {{currentFrameID}}\n    </p>\n    </div>\n    </div>\n    <a class=\"trigger\" href=\"#\">File Info</a> -->\n\n<!--     <HR>\n    <div class=\"AddedChange\">\n    <pre>\n        {{diffsArray.textAdded}}\n    </pre>\n    <HR>\n\t</div>\n\t<div class=\"RemovedChange\">\n    <pre>\n        {{diffsArray.textRemoved}}\n    </pre>\n    </div>\n    <HR>\n\t<div class=\"NoChange\">\n    <pre>\n        {{diffsArray.textSame}}\n    </pre>\n    <HR>\n\t</div>\n    <em>Current Frame</em>\n    <div ng-show=\"currentFrameID === 0\">\n    <em>First Frame</em>\n    <pre>\n    {{firstFrame}}\n    </pre>\n    </div>\n\t<pre>\n\t\t{{currentFrame}}\n\t</pre> -->\n\n</div>","event_type":"change","last_commit":"eb77fb2a012e56c6a3e694d1396b8768ad7c283b\n","last_commit_time":"1434909356\n","prev_keyframe":"GWIcgDCri0Br7sjG","next_keyframe":"XpnB49DSqRJlFe12","branch_name":"frontend-cleanup\n","createdAt":{"$$date":1434918356779},"_id":"3MAvDYOA8e5BgTEI"}
{"filename":"/Users/carolynyoo/Fullstack/gitplayback/js/dirTree.js","text_state":"var fs = require('fs'),\n    path = require('path'),\n    gitIgnore = path.join(process.env.PWD, \".gitignore\"); \n\n    var globsToIgnore = gitignoreParse(gitIgnore);\n    globsToIgnore.push('**/.git/**');\n\nfunction dirTree(filename) {\n    var stats = fs.lstatSync(filename),\n        info = {\n            \"path\": filename,\n            \"name\": path.basename(filename)\n        };\n\n    if (stats.isDirectory()) {\n        info.type = \"folder\";\n        info.children = fs.readdirSync(filename).map(function(child) {\n            return dirTree(filename + '/' + child);\n        });\n    } else {\n        // Assuming it's a file. In real life it could be a symlink or\n        // something else!\n        info.type = \"file\";\n    }\n\n    return info;\n}\n\nvar data = dirTree(process.env.PWD);\n\nmodule.exports = data;","event_type":"change","last_commit":"6be4d41916c196d72e3190220b51bf90d1688dd3\n","last_commit_time":"1434677800\n","prev_keyframe":"rEi56jN1S8m6lVAO","next_keyframe":"XCjEr8RYiEDda6Yt","branch_name":"dirtree\n","createdAt":{"$$date":1434822833663},"_id":"3ORrhw32ZFEyxJgp"}
{"filename":"/Users/carolynyoo/Fullstack/gitplayback/js/dirTree.js","text_state":"var fs = require('fs'),\n    path = require('path'),\n    gitignoreParse = require('gitignore-globs'),\n    minimatch = require('minimatch'); \n\nfunction skipReading (filename) {\n    var gitIgnore = path.join(process.env.PWD, \".gitignore\"); \n    var globsToIgnore = gitignoreParse(gitIgnore);\n    globsToIgnore.push('**/.git/**', '**/.gitignore');\n    console.log(globsToIgnore);\n\n    for (var i=0; i<globsToIgnore.length; i++) {\n      if (minimatch(filename, globsToIgnore[i])) {\n        return false;\n      }\n    }\n    return true;\n}\n\nfunction dirTree(filename) {\n    if (!skipReading(filename)) {\n        console.log(filename);\n        return;\n    }\n\n    var stats = fs.lstatSync(filename),\n        info = {\n            \"path\": filename,\n            \"name\": path.basename(filename)\n        };\n\n    if (stats.isDirectory()) {\n        info.type = \"folder\";\n        info.children = fs.readdirSync(filename).map(function(child) {\n            return dirTree(filename + '/' + child);\n        })\n        .filter(function (elem) {\n            console.log(elem);\n            if (elem) {\n                if (elem.children && elem.children.length===0) {\n                    return false;\n                } else {\n                    return true;\n                }\n            }\n            return false;\n        });\n    } else {\n        // Assuming it's a file. In real life it could be a symlink or\n        // something else!\n        info.type = \"file\";\n    }\n\n    return info;\n}\n\nvar arr = [];\nvar data = dirTree(process.env.PWD);\narr.push(data);\n\nmodule.exports = arr;","event_type":"change","last_commit":"b3c48824ab7612bee78388e199ad1378ea6e3a3f\n","last_commit_time":"1434823874\n","prev_keyframe":"7sZkDNnos5LPypA8","next_keyframe":"TnlZATBtLfaNI1mG","branch_name":"dirtree\n","createdAt":{"$$date":1434835614138},"_id":"3fCasOEEmYaJzm2H"}
